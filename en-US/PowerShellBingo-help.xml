			<command:parameter required = "false" variableLength = "false" globbing = "false" pipelineInput = "false" position = "named" >
				<maml:name>AutoPlay</maml:name>
				<maml:description>
					<maml:para>Have the computer automatically draw numbers and update the card until a
Bingo is made.</maml:para>
				</maml:description>
			</command:parameter>
			<command:parameter required = "false" variableLength = "false" globbing = "false" pipelineInput = "false" position = "named" >
				<maml:name>Sleep</maml:name>
